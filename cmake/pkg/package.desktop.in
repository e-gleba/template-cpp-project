[Desktop Entry]
Type=Application
Name=@PROJECT_NAME@
GenericName=@PROJECT_NAME@
Comment=@PROJECT_DESCRIPTION@
Exec=main %F
Icon=icon
Terminal=false
Categories=Utility;Development;
Keywords=cpp;utility;command-line;linux;
StartupNotify=true
Actions=About;Help;

[Desktop Action About]
Name=About @PROJECT_NAME@
Exec=xdg-open @PROJECT_HOMEPAGE_URL@

[Desktop Action Help]
Name=Get Help
Exec=xdg-open @PROJECT_HOMEPAGE_URL@
